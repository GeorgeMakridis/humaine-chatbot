{
    "name": "@humaine/human-centric-chatbot",
    "version": "0.0.1",
    "description": "Stencil Component Starter",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/human-centric-chatbot/human-centric-chatbot.esm.js",
    "exports": {
        ".": {
            "import": "./dist/human-centric-chatbot/human-centric-chatbot.esm.js",
            "require": "./dist/human-centric-chatbot/human-centric-chatbot.cjs.js"
        },
        "./humaine-chatbot": {
            "import": "./dist/components/humaine-chatbot.js",
            "types": "./dist/components/humaine-chatbot.d.ts"
        },
        "./loader": {
            "import": "./loader/index.js",
            "require": "./loader/index.cjs",
            "types": "./loader/index.d.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ionic-team/stencil-component-starter.git"
    },
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate"
    },
    "devDependencies": {
        "@stencil/core": "4.22.0",
        "@types/jest": "^29.5.6",
        "@types/node": "^16.18.11",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "puppeteer": "^21.9.0"
    },
    "license": "MIT",
    "dependencies": {
        "sentiment": "^5.0.2",
        "typo-js": "^1.2.4",
        "uuid": "^11.0.5"
    }
}
